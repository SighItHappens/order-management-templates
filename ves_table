 CREATE TABLE "VERIZON"."VES" 
   (	"MODEL_TYPE" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"CUSTOMER_ID" NUMBER NOT NULL ENABLE, 
	"CLASS_OF_SERVICE" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"FROM_DATE" DATE NOT NULL ENABLE, 
	"TO_DATE" DATE NOT NULL ENABLE, 
	"DISCOUNT_PERCENTAGE" NUMBER NOT NULL ENABLE, 
	"CONTRACT_ID" NUMBER NOT NULL ENABLE, 
	"CHANG" NUMBER, 
	"CURRENT" NUMBER, 
	"MAX" NUMBER, 
	 CONSTRAINT "VES_PK" PRIMARY KEY ("CONTRACT_ID")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS NOCOMPRESS LOGGING
  TABLESPACE "USERS"  ENABLE, 
	 CONSTRAINT "CHK_CLASS_OF_SERVICE" CHECK (
class_of_service in ('platinum','gold','silver','bronze','normal')
) ENABLE, 
	 CONSTRAINT "CHK_MODEL_TYPE" CHECK (
model_type in ('transactional','rtb')
) ENABLE, 
	 CONSTRAINT "VES_CUSTOMER_TABLE_FK1" FOREIGN KEY ("CUSTOMER_ID")
	  REFERENCES "VERIZON"."CUSTOMER_TABLE" ("CUSTOMER_ID") ENABLE
   ) SEGMENT CREATION DEFERRED 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
  TABLESPACE "USERS" ;
 
